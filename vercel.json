{
  "version": 2,
  "builds": [
    
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/pages|chunks|runtime|css|media)/.+",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    }
  ],
  "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
      
    }
  ]
}
